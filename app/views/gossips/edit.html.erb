<h1>Editer le gossip</h1>

<%= form_for @gossip do |f|%>

    <div class="form-group">

        <label>Titre du gossip</label>

        <%= f.text_field :title%>

        <label>Contenu du gossip</label>

        <%= f.text_area :content%>

    </div>
    <div class="form-group">

        <button type="submit">Modifier</button>
    
    </div>


<% end %>